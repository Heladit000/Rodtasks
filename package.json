{
  "name": "rodtasks",
  "version": "1.0.0",
  "description": "task manager made in React.js",
  "main": "webpack.config.dev.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack serve --config webpack.config.dev.js",
    "build": "webpack --config webpack.config.js",
    "check-updates": "npx npm-check-updates -u",
    "pm2-serve": "npm run build && serve ./dist/index.html 8080 --name react-app",
    "commit": "git add . && gitmoji -c",
    "commit-patch": "git add . && npm version patch --no-git-tag-version && gitmoji -c",
    "commit-minor": "git add . && npm version minor --no-git-tag-version && gitmoji -c",
    "commit-major": "git add . && npm version major --no-git-tag-version && gitmoji -c"
  },
