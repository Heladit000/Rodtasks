    "@babel/preset-react": "^7.18.6",
    "@svgr/webpack": "^6.5.1",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "babel-loader": "^9.1.2",
    "clean-terminal-webpack-plugin": "^3.0.0",
    "css-loader": "^6.7.3",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "ip": "^1.1.8",
